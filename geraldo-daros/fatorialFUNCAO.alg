algoritmo "fatorialFUNCAO"
var
   n, f : inteiro
   
funcao fatorial(v: inteiro) :inteiro
var
   c,r :inteiro
inicio
  r <- 1
  para c <- 1 ate v faca
       r <- r * c
  fimpara
  retorne R
fimfuncao
inicio
  escreva("Digite um numero: ")
  leia(n)
  f <- fatorial(n)
  escreval("O valor de  ", n, "! e igual a", f )
fimalgoritmo
