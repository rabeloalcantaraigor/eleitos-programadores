algoritmo "conta_negativos "
var
   n, c, totn:inteiro
inicio
    c <- 1
    totn <- 0
    repita
          escreva("Digite um numero: ")
          leia(n)
          Se (n<0) entao
             totn <- totn + 1
          fimse
          c <- c + 1
    ate(c>5)
    escreval("Foram digitados", totn, "valores negativos.")
fimalgoritmo