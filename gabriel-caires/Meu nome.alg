algoritmo "Meu nome"
var
   n1, n2, s: real
inicio
      escreval ("Vamos fazer uma soma?")
      escreval ("Informe o primeiro número")
      leia (n1)
      escreval ("Informe o segundo número")
      leia (n2)
      s <- n1+n2
      escreva ("A soma é entre",n1, " e",n2, " é igual a ", s)
      

fimalgoritmo
